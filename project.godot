config_version=5

[application]

config/name="Godot-TestProject-SteamInput"
run/main_scene="res://Scenes/main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

SavedVariables="*res://saved_variables.gd"
SteamworksGlyphBank="*res://SteamworksGlyphBank.gd"
SteamworksGdLoader="*res://SteamworksGDLoader.gd"
SteamworksInputLoader="*res://SteamworksInputLoader.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/borderless=true
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Godot-TestProject-SteamInput"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
